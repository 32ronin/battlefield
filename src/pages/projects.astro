<style lang="less">
  /*-- -------------------------- -->
<---          Gallery           -->
<--- -------------------------- -*/

/* Mobile - 360px */
@media only screen and (min-width: 0rem) {
    #gallery-434 {
        padding: var(--sectionPadding);

        .cs-container {
            width: 100%;
            margin: auto;
            display: flex;
            flex-direction: column;
            align-items: center;
            /* 48px - 64px */
            gap: clamp(3rem, 6vw, 4rem);
        }

        .cs-content {
            /* set text align to left if content needs to be left aligned */
            text-align: center;
            width: 100%;
            display: flex;
            flex-direction: column;
            /* centers content horizontally, set to flex-start to left align */
            align-items: center;
        }

        .cs-grid {
            display: grid;
            /* 12 so we can have rows of 3 and rows with 2 50/50 images */
            grid-template-columns: repeat(12, 1fr);
            /* 12px - 30px */
            grid-gap: clamp(0.75rem, 1.5vw, 1.875rem);
            grid-template-rows: repeat(6, 0.34fr);
        }

        .cs-picture {
            border-radius: (8/16rem);
            /* clips corners of img tags */
            overflow: hidden;
            position: relative;
            display: block;

            img {
                width: 100%;
                height: 100%;
                /* makes image behave like a background image */
                object-fit: cover;
            }
        }

        .cs-picture1 {
            display: none;
        }
        .cs-picture2 {
            grid-column: ~"1 / 9";
            grid-row: ~"1 / 3";
        }
        .cs-picture3 {
            grid-column: ~"9 / 13";
            grid-row: ~"1 / 1";
        }
        .cs-picture4 {
            grid-column: ~"1 / 7";
            grid-row: ~"4 / 5";
        }
        .cs-picture5 {
            grid-column: ~"7 / 13";
            grid-row: ~"4 / 6";
        }
        .cs-picture6 {
            display: none;
        }
        .cs-picture7 {
            grid-column: ~"1 / 5";
            grid-row: ~"3 / 4";
        }
        .cs-picture8 {
            grid-column: ~"5 / 9";
            grid-row: ~"3 / 4";
        }
        .cs-picture9 {
            grid-column: ~"9 / 13";
            grid-row: ~"2 / 4";
        }
        .cs-picture10 {
            grid-column: ~"1 / 7";
            grid-row: ~"5 / 7";
        }
        .cs-picture11 {
            grid-column: ~"7 / 13";
            grid-row: ~"6 / 7";
        }
    }
}

/* Tablet - 768px */
@media only screen and (min-width: 48rem) {
    #gallery-434 {
        padding-left: 0;
        padding-right: 0;
        .cs-grid {
            grid-template-columns: repeat(6, 1fr);
            grid-template-rows: repeat(3, 0.45fr);
        }
        .cs-picture1 {
            border-radius: 0 (8/16rem) (8/16rem) 0;
            display: block;
            grid-column: ~"1 / 3";
            grid-row: ~"1 / 2";
        }
        .cs-picture2 {
            grid-column: ~"3 / 5";
            grid-row: ~"1 / 3";
        }
        .cs-picture3 {
            grid-column: ~"5 / 6";
            grid-row: ~"1 / 1";
        }
        .cs-picture4 {
            border-radius: (8/16rem) 0 0 (8/16rem);
            grid-column: ~"6 / 7";
            grid-row: ~"1 / 1";
        }
        .cs-picture5 {
            display: none;
        }
        .cs-picture6 {
            border-radius: 0 (8/16rem) (8/16rem) 0;
            display: block;
            grid-column: ~"1 / 3";
            grid-row: ~"2 / 4";
        }
        .cs-picture7 {
            grid-column: ~"3 / 4";
            grid-row: ~"3 / 4";
        }
        .cs-picture8 {
            grid-column: ~"4 / 5";
            grid-row: ~"3 / 4";
        }
        .cs-picture9 {
            grid-column: ~"5 / 6";
            grid-row: ~"2 / 4";
        }
        .cs-picture10 {
            border-radius: (8/16rem) 0 0 (8/16rem);
            grid-column: ~"6 / 7";
            grid-row: ~"2 / 4";
        }
        .cs-picture11 {
            display: none;
        }
    }
}

/* Large Desktop 1600px */
@media only screen and (min-width: 100rem) {
    #gallery-434 {
        .cs-grid {
            grid-template-columns: repeat(7, 1fr);
            grid-template-rows: repeat(3, 0.4fr);
        }

        .cs-picture4 {
            border-radius: (8/16rem);
        }

        .cs-picture5 {
            border-radius: (8/16rem) 0 0 (8/16rem);
            display: block;
            grid-column: ~"7 / 8";
            grid-row: ~"1 / 3";
        }

        .cs-picture10 {
            border-radius: (8/16rem);
        }

        .cs-picture11 {
            border-radius: (8/16rem) 0 0 (8/16rem);
            display: block;
            grid-column: ~"7 / 8";
            grid-row: ~"3 / 4";
        }
    }
}

                                
</style>

